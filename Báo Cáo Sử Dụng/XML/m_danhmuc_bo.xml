		<xs:element name="NewDataSet" msdata:IsDataSet="true">
			<xs:complexType>
				<xs:choice maxOccurs="unbounded">
					<xs:element name="Table">
						<xs:complexType>
							<xs:sequence>
                <xs:element name="SQL" type="xs:string" minOccurs="0" />
                <xs:element name="MA" type="xs:string" minOccurs="0" />
                <xs:element name="TEN" type="xs:string" minOccurs="0" />
              </xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:complexType>
		</xs:element>
	</xs:schema>
	<Table>
		<SQL>select makp as ma,tenkp as ten from btdkp order by makp</SQL>
